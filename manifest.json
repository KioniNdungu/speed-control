{
  "manifest_version": 3,
  "name": "Speed Control",
  "version": "1.0",
  "description": "Alter video speed and remember settings.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["storage", "activeTab"]
}
